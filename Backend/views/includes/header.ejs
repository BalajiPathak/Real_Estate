<%# include('head.ejs') %>

<div id="preloader">
    <div id="status">&nbsp;</div>
</div>
<!-- Body content -->
<div class="header-connect">
    <div class="container">
        <div class="row mobile-header">
            <div class="col-md-5 col-sm-8 col-xs-12">
                <div class="header-half header-call">
                    <p>
                        <% if (companyInfo) { %>
                            <span><i class="pe-7s-call"></i> <%= companyInfo.Phone_No %></span>
                            <span><i class="pe-7s-mail"></i> <%= companyInfo.Email %></span>
                        <% } %>
                    </p>
                </div>
            </div>
            <div class="col-md-2 col-md-offset-5 col-sm-3 col-sm-offset-1 col-xs-12">
                <div class="header-half header-social">
                    <ul class="list-inline social-icons">
                        <% if (companyInfo) { %>
                            <% if (companyInfo.facebook) { %>
                                <li><a href="<%= companyInfo.facebook %>"><i class="fa fa-facebook"></i></a></li>
                            <% } %>
                            <% if (companyInfo.twitter) { %>
                                <li><a href="<%= companyInfo.twitter %>"><i class="fa fa-twitter"></i></a></li>
                            <% } %>
                            <% if (companyInfo.Instagram) { %>
                                <li><a href="<%= companyInfo.Instagram %>"><i class="fa fa-instagram"></i></a></li>
                            <% } %>
                            <% if (companyInfo.linkedIn) { %>
                                <li><a href="<%= companyInfo.linkedIn %>"><i class="fa fa-linkedin"></i></a></li>
                            <% } %>
                        <% } %>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>




<!-- Remove this line as it causes circular reference -->
<%# include('end.ejs') %>