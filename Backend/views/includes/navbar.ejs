<nav class="navbar navbar-default">
    <div class="container">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navigation">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <% if (companyInfo && companyInfo.Logo) { %>
                <a class="navbar-brand" href="/home">
                    <img src="<%= companyInfo.Logo %>" alt="Company Logo">
                </a>
            <% } %>
        </div>
        <div class="collapse navbar-collapse yamm" id="navigation">
            <div class="button navbar-right">
                <% if (!isLoggedIn) { %>
                    <a href="/login"> 
                        <button class="navbar-btn nav-button wow bounceInRight login" data-wow-delay="0.45s">Login</button>
                    </a>
                    <a href="/signup"> 
                        <button class="navbar-btn nav-button wow fadeInRight" data-wow-delay="0.48s">Signin</button>
                    </a>
                <% } else { %>
                    <form action="/logout" method="post" style="display: inline;">
                        <button type="submit" class="navbar-btn nav-button wow bounceInRight login" data-wow-delay="0.45s">Logout</button>
                    </form>
                    <a href="/property/new"> 
                        <button class="navbar-btn nav-button wow fadeInRight" data-wow-delay="0.48s">Submit Your Property</button>
                    </a>
                <% } %>
            </div>
            <ul class="main-nav nav navbar-nav navbar-right">
                <% if (navbar && navbar.length > 0) { %>
                    <% navbar.forEach(function(nav) { %>
                        <li class="dropdown ymm-sw" data-wow-delay="0.1s">
                            <a href="/<%= nav.Navbar_Name.toLowerCase() %>" data-hover="dropdown">
                                <%= nav.Navbar_Name %>
                            </a>
                        </li>
                    <% }); %>
                <% } else { %>
                    <li><a href="#">No items available</a></li>
                <% } %>
            </ul>
        </div>
    </div>
</nav>
