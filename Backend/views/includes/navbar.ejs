                        <button type="submit" class="navbar-btn nav-button wow bounceInRight login" data-wow-delay="0.45s">Logout</button>
                    </form>
                     <a href="/purchase-history"> 
                        <button class="navbar-btn nav-button wow fadeInRight" data-wow-delay="0.48s">See Your Purchase History</button>
                    </a>
