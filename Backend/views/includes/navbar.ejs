                                                    <ul>
                                                        <li><a href="/home">Home Style 1</a></li>
                                                    </ul>
                                                </div>
                                                <div class="col-sm-3">
                                                    <h5>Pages and blog</h5>
                                                    <ul>
                                                        <li><a href="/blog">Blogs</a></li>
                                                        <li><a href="/contact">Contact</a></li>
                                                        <li><a href="/faqs">FAQ page</a></li>
